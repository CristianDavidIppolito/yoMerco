{
  "name": "MyUser",
  "plural": "my-users",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": true
    }
  },
  "properties": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "roles": {
      "type": "hasMany",
      "model": "MyRole",
      "foreignKey": "userId",
      "through": "MyRoleMyUser"
    },
    "addresses": {
      "type": "hasMany",
      "model": "Address",
      "foreignKey": "userId"
    },
    "providers": {
      "type": "hasMany",
      "model": "Provider",
      "foreignKey": "userId"
    },
    "orders": {
      "type": "hasMany",
      "model": "Order",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {}
}
