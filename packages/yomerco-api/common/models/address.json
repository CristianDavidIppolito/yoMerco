{
  "name": "Address",
  "plural": "addresses",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": true
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "value": {
      "type": "string",
      "required": true
    },
    "main": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "MyUser",
      "foreignKey": "userId"
    },
    "city": {
      "type": "belongsTo",
      "model": "City",
      "foreignKey": "cityId"
    }
  },
  "acls": [],
  "methods": {}
}
